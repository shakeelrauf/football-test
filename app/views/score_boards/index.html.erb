<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Team</th>
      <th scope="col">P</th>
      <th scope="col">W</th>
      <th scope="col">L</th>
      <th scope="col">D</th>
      <th scope="col">F</th>
      <th scope="col">A</th>
      <th scope="col">Pts</th>
    </tr>
  </thead>
  <tbody>
    <% @score_boards.each.with_index do |sb, i| %>
      <tr>
        <th scope="row"><%= i + 1 %></th>
        <td><%= sb.team %></td>
        <td><%= sb.P %></td>
        <td><%= sb.W %></td>
        <td><%= sb.L %></td>
        <td><%= sb.D %></td>
        <td><%= sb.F %></td>
        <td><%= sb.A %></td>
        <td><%= sb.Pts %></td>
      </tr>
    <% end %>    
  </tbody>
</table>